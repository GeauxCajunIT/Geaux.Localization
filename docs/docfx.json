{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Geaux.Localization.csproj"
          ],
          "src": "../src"
        }
      ],
      "exclude": [
        "**/bin/**",
        "**/obj/**",
        "sample/**",
        "tests/**"
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net9.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "index.md",
          "toc.yml"
        ]
      },
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Geaux.Localization",
      "_enableSearch": true
    },
    "template": [
      "default",
      "modern"
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ]
  }
}